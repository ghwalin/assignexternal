<div class="container-fluid tertiary-navigation">
    <div class="row">
        <div class="navitem">
            <a class="btn btn-secondary mr-1" href="{{link_grading}}">{{#str}}showall, core{{/str}}</a>
        </div>
        <div class="navitem">
            <a class="btn btn-primary" href="{{link_grader}}">{{#str}}grading, mod_assignexternal{{/str}}</a>
        </div>
    </div>
</div>
<div class="gradingsummary">
    <h3>Grading overview</h3>
    <div class="box py-3 boxaligncenter gradingsummarytable">
        <div class="table-responsive">
            <table class="generaltable table-bordered">
                <tbody>
                <tr class="">
                    <th class="cell c0" style="" scope="row">{{#str}}hiddenfromstudents{{/str}}</th>
                    <td class="cell c1 lastcol" style="">{{hidden}}</td>
                </tr>
                <tr class="">
                    <th class="cell c0" style="" scope="row">{{#str}}students, core{{/str}}</th>
                    <td class="cell c1 lastcol" style="">{{student_count}}</td>
                </tr>
                <tr class="">
                    <th class="cell c0" style="" scope="row">{{#str}}graded, mod_assignexternal{{/str}}</th>
                    <td class="cell c1 lastcol" style="">{{graded_count}}</td>
                </tr>
                <tr class="lastrow">
                    <th class="cell c0" style="" scope="row">{{#str}}timeremaining, mod_assignexternal{{/str}}</th>
                    <td class="cell c1 lastcol" style="">{{timeremaining}}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>